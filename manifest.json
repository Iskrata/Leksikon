{
    "manifest_version": 3,
  
    "name": "Leksikon",
    "description": "Тази наставка намира граматични, пунктуационни и правописни грешки.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "Licon.png",
     "default_title": "Провери сега",
     "default_popup": "popup.html"
    },

    "optional_permissions": [
        "tabs",
        "topSites",
        "<all_urls>"
     ],

    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "<all_urls>"
    ]
  }