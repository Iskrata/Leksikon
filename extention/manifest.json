{
    "manifest_version": 3,
  
    "name": "Leksikon",
    "description": "This extension will search for punctuation, grammatical and spelling mistakes.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab",
     "tabs",
     "<all_urls>"
     ]
  }